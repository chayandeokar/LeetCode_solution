   "source": [
    "number = int(input())\n",
    "sum = 0\n",
    "temp = number\n",
    "\n",
    "while temp > 0:\n",
    "    digit = temp % 10\n",
    "    sum += digit ** 3\n",
    "    temp //= 10\n",
    "    \n",
    "if number == sum:\n",
    "    print(number,\"is an Armstrong Number\")\n",
    "else:\n",
    "    print(number,\"not an armstrong number\")"
   ]
